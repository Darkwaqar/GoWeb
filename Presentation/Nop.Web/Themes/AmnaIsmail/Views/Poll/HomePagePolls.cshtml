@model IList<PollModel>
@using growonline.Web.Models.Polls;
@if (Model.Count > 0)
{
    <div class="page-header text-center">
        <h3>@T("Polls.Title")</h3>
    </div>
    <div class="row">
        @foreach (var poll in Model)
        {
            @Html.Partial("_Poll", poll)
        }
    </div>
}
