@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";

    //page class
    Html.AppendPageCssClassParts("html-home-page");
    Html.AddCssFileParts("~/Themes/Preta/Content/css/mainpage.css");
    Html.AddScriptParts("~/Themes/Preta/Content/assets/js/app.js");
}
<div class="container" id="articals">
    <div class="row">
        <div class="col-lg-9 col-md-9 col-sm-9 editor-pick grid">
            @Html.Widget("Editor-Pick")
            <div class="col-md-3 col-sm-6 col-xs-6 grid-item">
                <img src="~/Themes/Preta/Content/img/banner1.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-9 col-sm-12 col-xs-12 grid-item padding-top">
                <img src="~/Themes/Preta/Content/img/banner2.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-12 col-sm-12 col-xs-12 grid-item padding-top">
                <img src="http://homistan.com/gfh/assets/img/home/3.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-6 col-sm-6 col-xs-12 grid-item padding-top">
                <img src="~/Themes/Preta/Content/img/banner4.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-6 col-sm-6 col-xs-12  grid-item padding-top">
                <img src="~/Themes/Preta/Content/img/banner5.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-3 col-sm-6 col-xs-6 grid-item padding-top">
                <img src="~/Themes/Preta/Content/img/banner6.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-md-9 col-sm-12 col-xs-12  grid-item padding-top">
                <img src="~/Themes/Preta/Content/img/banner7.png" class="img-responsive" alt="Alternate Text" />
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6  margin-top positon-relative">
                <div class=" text-center">
                    <img style="max-width:100%" src="//cdn.thinglink.me/api/image/1060168556051693570/1024/10/scaletowidth#tl-1060168556051693570;'" class="alwaysThinglink" />
                    <script async charset="utf-8" src="//cdn.thinglink.me/jse/embed.js"></script>
                    <div class="main-page-detail">
                        <h6 class="product-short-desc-2 no-margin ">
                            Expert advice
                        </h6>
                        <h6 class="product-short-desc-2  ">
                            Discover the secret beauty rituals of an A-list makeup artist
                        </h6>
                        <p class="text-link-story ">
                            Read & shop
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6  margin-top positon-relative ">
                <div class="text-center">
                    <img style="max-width:100%" src="//cdn.thinglink.me/api/image/1059957462150414338/1024/10/scaletowidth#tl-1059957462150414338;'" class="alwaysThinglink" />
                    <div class="main-page-detail">
                        <h6 class="product-short-desc-2 no-margin ">
                            The right track
                        </h6>
                        <h6 class="product-short-desc-2 ">
                            Discover the secret beauty rituals of an A-list makeup artist
                        </h6>
                        <p class="text-link-story ">
                            Read & shop
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-xs-6 margin-top ">
                @Html.Widget("main_big_slider_three_1")
            </div>
            <div class="col-md-6 col-xs-6 margin-top">
                @Html.Widget("main_big_slider_three_2")
            </div>
            <div class="col-md-12 margin-top">
                @Html.Widget("main_big_slider_four")
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-md-12 margin-top">
               @* @Html.Action("HomepageCategories", "Catalog")*@
                @Html.Action("HomepageCollection", "Catalog")
            </div>
        </div>
        <div class="col-md-3 col-sm-3 col-lg-3 text-center no-padding right-side">
            <div class="border" id="subscribe">
                <h6 class="text-bridal-banner">Prep - Coming Soon</h6>
                <img src="~/Themes/Preta/Content/assets/img/home/collection.png" class="img-responsive center-block">
                <br />
                <h3 class="text-bridal-banner">Launching Soon</h3>
                <div class="col-md-12">
                    <p id="countdown_timer"></p>
                </div>
                <div class="row margin-ten-px">
                    <div class="col-sm-12 col-md-9 no-padding" style="">
                        <input type="email" class="newsletter-email-box" placeholder="email">
                    </div>
                    <div class="col-sm-12 col-md-3 padding-bottom  no-padding ">
                        <button class="subscribe-button-red"> Let's Go</button>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 padding-left t1-rightcolumn margin-top no-padding" >
                @Html.Widget("home_page_bottom")
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $('#countdown').countdown('2017/04/28', function (event) {
            $(this).html(event.strftime('%d : %H : %M : %S '));
        });

        function calculateVisibilityForDiv() {
            var div$ = $(".mf-footer:first");
            var windowHeight = $(window).height(),
                docScroll = $(document).scrollTop(),
                divPosition = div$.offset().top,
                divHeight = div$.height(),
                hiddenBefore = docScroll - divPosition,
                hiddenAfter = (divPosition + divHeight) - (docScroll + windowHeight);

            if ((docScroll > divPosition + divHeight) || (divPosition > docScroll + windowHeight)) {
                return 0;
            } else {
                var result = 100;

                if (hiddenBefore > 0) {
                    result -= (hiddenBefore * 100) / divHeight;
                }

                if (hiddenAfter > 0) {
                    result -= (hiddenAfter * 100) / divHeight;
                }
                return result;
            }
        }

        function fixDiv() {
            divWidth = $("#articals").width() / 4;
            var $cache = $('.t1-rightcolumn');
            var divTop = $("#subscribe").offset().top + $("#subscribe").height();
            var divBottom = $("#footer").offset().top;
            if ($(window).scrollTop() > divTop  && calculateVisibilityForDiv() <= 0) {
                $cache.css({
                    'position': 'fixed',
                    'top': '0px',
                    'width': divWidth + "px",
                    'height': $(window).height(),
                    'margin-top': '0',
                });
            }
            else if (calculateVisibilityForDiv() > 0) {
                var topPos = divBottom - $cache.height() - $("#subscribe").offset().top- $("#subscribe").height()- 50;
                $cache.css({
                    'position': 'relative',
                    'top': topPos,
                    'width': divWidth + "px",
                    'margin-top': '0',
                    'height': $(window).height(),
                    'right': 'auto'
                });
            }
            else {
                $cache.css({
                    'position': 'relative',
                    'top': 'auto',
                    'width': divWidth + "px",
                    'margin-top': '20px',
                    'height': $(window).height(),
                    'right': 'auto'
                });
            }
        }

        $(window).scroll(function () {
            if ($(window).width() > 768) {
                fixDiv();
                $('.right-side').css("clear", "");
            }
            else {
                $('.t1-rightcolumn').width("95%");
                $('.right-side').css("clear","both");
            }
        });

        
       
        
