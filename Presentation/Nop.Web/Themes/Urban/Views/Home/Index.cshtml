@{
    Layout = "~/Views/Shared/_ColumnsOne.cshtml";
//page class
    Html.AppendPageCssClassParts("html-home-page");
    if (!TempData.ContainsKey("IsHomePage"))
    {
        TempData.Add("IsHomePage", true);
    }
}
<div class="page home-page">
    <div class="page-body">
        @Html.Widget("home_page_top")
        @Html.Widget("home_page_before_categories")
        @Html.Action("HomepageCategories", "Catalog")
        @Html.Widget("home_page_before_products")
        @Html.Widget("home_page_urban_categories")
        @*@Html.Action("HomepageProducts", "Product")*@
        @Html.Action("TopicBlock", "Topic", new { systemName = "HomePageText" })
       
        @*@Html.Widget("home_page_before_news")
        @Html.Action("HomePageNews", "News")*@
        @Html.Widget("home_page_before_best_sellers")
        @*@Html.Action("HomepageBestSellers", "Product")*@
        @Html.Widget("home_page_before_poll")
	<div class="HomePage-Video item-grid">
           <video id="myVideo"playsinline autoplay muted loop>
                <source src="~/Themes/Urban/Content/Video/ezgif.com-video-to-mp4.mp4" type='video/mp4;codecs="avc1.42E01E, mp4a.40.2"' />
                Your browser does not support the video tag.
            </video>

        </div>
        @Html.Action("HomePagePolls", "Poll")
        @Html.Widget("home_page_bottom")
    </div>
</div>
<script>
    $(window).ready(function () {
        myVideo.play(); 
    })
</script>