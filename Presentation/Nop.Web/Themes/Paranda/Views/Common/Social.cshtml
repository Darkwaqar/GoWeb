@using Nop.Core
@using Nop.Core.Infrastructure
@using Nop.Services.Configuration
@model Nop.Web.Models.Common.SocialModel
@{
    var storeId = EngineContext.Current.Resolve<IStoreContext>().CurrentStore.Id;
    var settingService = EngineContext.Current.Resolve<ISettingService>();
    var instagramUrl = settingService.GetSettingByKey("pavilionthemesettings.instagramurl", String.Empty, storeId, true);
    var pinterestUrl = settingService.GetSettingByKey("pavilionthemesettings.pinteresturl", String.Empty, storeId, true);
    var vimeoUrl = settingService.GetSettingByKey("pavilionthemesettings.vimeourl", String.Empty, storeId, true);
}
<ul class="social">


    @if (!String.IsNullOrEmpty(Model.TwitterLink))
    {
        <li class="twitter">
            <a href="@Model.TwitterLink" target="_blank" title="Twitter" class="tooltipOn">
                <i class="fa fa-twitter fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(Model.FacebookLink))
    {
        <li class="facebook">
            <a href="@Model.FacebookLink" target="_blank" title="facebook" class="tooltipOn">
                <i class="fa fa-facebook fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(instagramUrl))
    {
        <li class="instagram">
            <a href="@instagramUrl" target="_blank" title="instagram" class="tooltipOn">
                <i class="fa fa-instagram fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(pinterestUrl))
    {
        <li class="pinterest">
            <a href="@pinterestUrl" target="_blank" title="pinterest" class="tooltipOn">
                <i class="fa fa-pinterest fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(Model.YoutubeLink))
    {
        <li class="youtube">
            <a href="@Model.YoutubeLink" target="_blank" title="youtube" class="tooltipOn">
                <i class="fa fa-youtube fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(Model.GooglePlusLink))
    {
        <li class="google-plus">
            <a href="@Model.GooglePlusLink" target="_blank" title="Google+" class="tooltipOn">
                <i class="fa fa-google-plus fa-2x"></i>
            </a>
        </li>
    }
    @if (!String.IsNullOrEmpty(vimeoUrl))
    {
        <li class="vimeo">
            <a href="@vimeoUrl" target="_blank" title="vimeo" class="tooltipOn">
                <i class="fa fa-vimeo fa-2x"></i>
            </a>
        </li>
    }
    @if (Model.NewsEnabled)
    {
        <li title="Rss" class="tooltipOn" facebook="rss"><a target="_blank" href="@Url.RouteUrl("NewsRSS", new {languageId = Model.WorkingLanguageId})"><i class="fa fa-rss fa-2x"></i></a></li>
    }
</ul>