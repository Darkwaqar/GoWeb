@model IList<CategoryModel>
@using Nop.Web.Models.Catalog;
@using Nop.Core.Infrastructure
@using Nop.Web.Framework.Themes
@{
    var themeName = EngineContext.Current.Resolve<IThemeContext>
    ().WorkingThemeName;
    var css = string.Format("Themes/{0}/Content/css", themeName);
    var js = string.Format("Themes/{0}/Content/js", themeName);
    var img = string.Format("Themes/{0}/Content/images", themeName);
    var video = string.Format("Themes/{0}/Content/Video", themeName);
}
@Html.Widget("body_end_html_tag_News")
<div style="position: relative; overflow: hidden;" class="mos-container isotope scroll-cont" >
    @if (Model.Count > 0)
    {
        int counter = 0;
        int numberOfDiv = 5;
        <div class="row text-center">
            @foreach (var item in Model)
            {
                if (counter % numberOfDiv == 0)
                {
                    <div class="mos-item block-2428 item-w2 item-sm-w2 item-sm-h2 img-center-horizontal slide-box item-width ">
                        @Html.Widget("paranda_home_pom_pom_catagory")
			@Html.Widget("home_page_Mobile_Jewellery_banner")
                        <div class="mos-panel animate ">
				@* @Url.RouteUrl("Product", new { SeName = item.SeName }) *@
                            <a class="mos-inner" href="@Url.RouteUrl("Product", new { SeName = item.SeName })">
                               @* <i style="padding-top:20px;" class="fa fa-chevron-right pull-right mobile-obj"></i>*@
                                <h2 class="mob-tit tit-slide-box">@item.Name</h2>
                                <div class="sep-line animate "></div>
                                <div id="catimg" >
                                   @Html.Raw(@item.Description)
                                </div>
                                <button class="btn btn-white-trasp">discover</button>
                            </a>
                        </div>
                    </div>
                }
                else if (counter % numberOfDiv == 1)
                {
                    <div class="mos-item block-2569 item-h2 item-sm-w2 item-sm-h2 slide-box home_banner">
                            @Html.Widget("paranda_home_banner")
			    @Html.Widget("home_page_Mobile_banner")
