                            </a>
                        </div>
                    </div>
                }

                else if (counter % numberOfDiv == 4)
                {
                    <div class="mos-item block-2449 item-df item-sm-w2 item-sm-h2 slide-box item-width">
                        @Html.Widget("paranda_home_accessories_catagory")
