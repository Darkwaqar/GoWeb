@model LogoModel
@using Nop.Web.Models.Common;

@{
    
    var tiffanyLogo = @Html.Widget("tiffany_theme_logo");
}


<div class="header-logo">
    <a href="@Url.RouteUrl("HomePage")" class="logo">
        @if (!MvcHtmlString.IsNullOrEmpty(tiffanyLogo))
        {
            @tiffanyLogo
        }
        else
        {
            
            <img title="" alt="@Model.StoreName" src="@Model.LogoPath">
        }
    </a>
</div>