@using Nop.Core
@using Nop.Core.Infrastructure
@using Nop.Services.Configuration

@model Nop.Web.Models.Common.SocialModel

@{
    var storeId = EngineContext.Current.Resolve<IStoreContext>().CurrentStore.Id;
    var pinterestUrl = EngineContext.Current.Resolve<ISettingService>().GetSettingByKey("tiffanythemesettings.pinteresturl", String.Empty, storeId, true);
    var vimeoUrl = EngineContext.Current.Resolve<ISettingService>().GetSettingByKey("tiffanythemesettings.vimeourl", String.Empty, storeId, true);
    var instagramUrl = EngineContext.Current.Resolve<ISettingService>().GetSettingByKey("tiffanythemesettings.instagramurl", String.Empty, storeId, true);
}

<ul class="networks">
	@if (!string.IsNullOrEmpty(Model.TwitterLink))
    {
        <li class="twitter"><a href="@Model.TwitterLink" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.FacebookLink))
    {
        <li class="facebook"><a href="@Model.FacebookLink" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.YoutubeLink))
    {
        <li class="youtube"><a href="@Model.YoutubeLink" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.GooglePlusLink))
    {
        <li class="google"><a href="@Model.GooglePlusLink" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(pinterestUrl))
    {
        <li class="pinterest"><a href="@pinterestUrl" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(vimeoUrl))
    {
        <li class="vimeo"><a href="@vimeoUrl" target="_blank"></a></li>
    }
    @if (!string.IsNullOrEmpty(instagramUrl))
    {
        <li class="instagram"><a href="@instagramUrl" target="_blank"></a></li>
    }
   @* @if (Model.NewsEnabled)
    {
        <li class="rss"><a target="_blank" href="@Url.RouteUrl("NewsRSS", new {languageId = Model.WorkingLanguageId})"></a></li>
    }*@
</ul>