    display:none;
}


#quickTabs {

}
.currency-selector {
    position: relative;
}


.currency-selector::after {
    content: '';
    position: absolute;
    top: 13px;
    right: 10px;
    width: 7px;
    height: 4px;
    overflow: hidden;
    background: url(../img/header-sprite.png) no-repeat -300px 0;
    border: 0 none;
}

.currency {
    position:relative;
}

.currency::after {
    content: '';
    position: absolute;
    top: 15px;
    right: -15px;
    width: 10px;
    height: 10px;
    overflow: hidden;
    background: url(../img/header-sprite.png) no-repeat -4px -103px;
    border: 0 none;
 
}

.header-inner {
    position:relative;
}

.header-inner .icon-box {
    float: left;
    width: 20%;
    padding-top: 20px;
    text-align:right;
   
}

.icon-box .flyout-cart {
    background: #fff;
    border: 1px solid #ccc;
    box-shadow: 0 0 3px #ccc;
    right:1%;
    padding: 20px;
    position: absolute;
    top: 70px;
    width: 29%;
