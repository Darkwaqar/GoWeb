.topbar {
    position: relative;
    height: 36px;
    width: 100%;
    border-bottom: 1px solid #e9e9e9;
    background-color: #000;
    z-index: 22;
    font-size: 11px;
    color: #cfcfcf;
}


.flag {
    display: inline-block;
    width:20px;
    min-width:20px;
    vertical-align:middle;
}

.topbar .topbar-inner, .header-inner, .page-content {
    display: block;
    font-family: 'Open Sans', sans-serif;
    width: 90%;
    margin: 0px auto;
}

    .header-inner {
        height:100px;
        padding:10px;
     }

        .header-inner .logo-placeholder {
            float:left; 
            width:20%;
            padding-top:15px;
        }

.logo-placeholder .home-main {
    width: 200px;
    display: block;
    text-align: center;
    text-decoration: none;

}

.logo-placeholder .logo {
    background: url(../img/logo-01.png) no-repeat;
    background-size:contain;
   
    height:45px;
    display: block;
}

.logo-placeholder .moto {
    font-size: 10px;
    display: block;
    color: #000;
    margin-top:-10px;
}




.header-inner .search-box {
    float: left;
    width: 60%;
    display: block;
    margin-top:20px;
}


.search-box .header-search {
    border: 1px solid #c3c3c3;
  
    height: 40px;
    padding: 10px;
    margin:0 auto;
    width:500px;
    position:relative;
    
}


.search-input {
    float: left;
    background-color: transparent;
    border: none !important;
    color: #000;
    width:100%;
    margin-top:-2px;
}

input:focus {
    outline: none;
}

.search-button {
    width: 40px;
    position: absolute;
    height: 40px;
    border: none;
    background: url(../img/header-sprite.png) no-repeat -593px 6px;
    background-color: #000;
    color: #fff;
    top: -1px;
    right:-1px;
}






        .topbar .topbar-inner .left {
            float:left;
        }

        .topbar .topbar-inner .right {
            float: right;
        }

    .topbar .topbar-menu {

    }

        .topbar .topbar-menu ul {
        }

            .topbar .topbar-menu > li {
                position: relative;
                float: left;
                padding: 0 15px;
                height: 35px;
                line-height: 35px;
                border-right: 1px solid #808080;
            }

                .topbar .topbar-menu > li a {
                    color:#cfcfcf;

                }



.switcher {
    cursor:pointer;
    padding-right:25px !important;
}




    .switcher:hover {
        margin: -1px -1px 0 -1px;
        height: 39px;
        background-color: #fff;
        border: 1px solid #ccc;
        border-bottom: 0 none;
        z-index: 21;
        box-shadow: 0 -3px 5px 0 rgba(51,51,51,.3);
        color:#808080;
    }


.switcher:hover  .switcher-sub {
    display: block;
}

        .switcher:hover .switcher::after {
            display:block;
        }

 .switcher::after {
            display: none;
            content: '';
            position: absolute;
            right: 3px;
            top: 34px;
            z-index: 14;
            width: 100%;
            height: 5px;
            background-color: #fff;
  }
        




        .switcher:hover .currency::after {
            background: url(../img/header-sprite.png) no-repeat -347px -0px;
        }

.switcher-sub {
    color:#808080;
    display: none;
    position: absolute;
    top: 34px;
    left: -1px;
    width: 300px;
    cursor:default;
    font-size: 11px;
    line-height: 1.3;
    background: #fff;
    z-index: 20;
    box-shadow: 0 2px 5px 0 rgba(51,51,51,.3);
}

    .switcher-sub .switcher-common {
        z-index: 999;
        padding: 10px 20px 15px;
        border-bottom: 1px solid #e8e8e8;
    }


        .switcher-sub .switcher-common .text {
            font-size:11px;
            padding-top:5px;
            padding-bottom:5px;
        }
        .switcher-sub .switcher-common .title {
            display: block;
            padding: 10px 0;
            color: #666;
            border-bottom: 1px solid #E9E9E9;
        }

        .switcher-sub .switcher-common .currency-selector{
            min-height: 30px;
            width: 100% !important;
        }

.switcher-sub select {
    -webkit-appearance: button;
    
    -webkit-padding-end: 20px;
    -webkit-padding-start: 2px;
    -webkit-user-select: none;
   
    background-repeat: no-repeat;
    color: #555;
    font-size: inherit;
    overflow: hidden;
    padding: 5px 10px;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;

    height: 30px;
    border: 1px solid #D6D6D6;


}


select {
    -webkit-appearance: button;
    -webkit-padding-end: 20px;
    -webkit-padding-start: 2px;
    -webkit-user-select: none;
    background-repeat: no-repeat;
    color: #555;
    font-size: inherit;
    overflow: hidden;
    padding: 5px 10px;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
