a {
    outline: none;
    text-decoration: none;
}

    a:focus, a:hover {
        text-decoration: none;
        color: #fff;
    }

button, .btn {
    box-shadow: none;
    text-decoration: none;
    outline: none;
    border: none;
}

input, input:focus {
    outline: none;
}

button:hover, button:focus, button:active, button:visited, .btn:hover, .btn:focus, .btn:active, .btn:visited, .btn:active:focus, button:active:focus {
    box-shadow: none;
    text-decoration: none;
    outline: none;
    border: none;
}

