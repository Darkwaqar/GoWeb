        color: #fff;
        outline: 0;
        border: 2px solid #f4c57d;
    }

.fifth {
    border-color: #f4c57d;
    border: 2px solid #f4c57d;
    border-radius: 0;
    color: #f4c57d;
    position: relative;
    overflow: hidden;
    z-index: 1;
    transition: color 150ms ease-in-out;
}


.set1 {
    margin:0px !important; 
}



button.btn1.fifth.font-24.set1:after {
    content: '';
    position: absolute;
    display: block;
    top: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 100%;
    background: #000;
    z-index: -1;
    transition: width 150ms ease-in-out;
}


button.btn1.fifth.font-24.set1 {
    border-color: #000;
    color: #000;
 
}

    button.btn1.fifth.font-24.set1:hover {
        color: #f4c37e;
    }
    button.btn1.fifth.font-24.set1:hover:after {
        width:110%;
    }

    .fifth:after {
        content: '';
        position: absolute;
        display: block;
        top: 0;
        left: 50%;
        transform: translateX(-50%);
        width: 0;
        height: 100%;
        background: #f4c57d;
        z-index: -1;
        transition: width 150ms ease-in-out;
    }
     .fifth:hover {
            color: #000;
        }
     .fifth:hover:after
{
    width: 110%;
}


#slider12  {
    width:80% !important;
    margin:0px auto;
}

[type=radio] {
    display: none;
}

#slider12 {
    height: 35vw;
    position: relative;
    perspective: 1000px;
    transform-style: preserve-3d;
}

    #slider label {
        margin: auto;
        width: 100%;
        height: 100%;
        border-radius: 0px;
        position: absolute;
        left: 0;
        right: 0;
        cursor: pointer;
        transition: transform 0.4s ease;
    }

#s1:checked ~ #slide4, #s2:checked ~ #slide5,
#s3:checked ~ #slide1, #s4:checked ~ #slide2,
#s5:checked ~ #slide3 {
    box-shadow: 0 1px 4px 0 rgba(0,0,0,.37);
    transform: translate3d(-100%,0,-200px);
}

#s1:checked ~ #slide5, #s2:checked ~ #slide1,
#s3:checked ~ #slide2, #s4:checked ~ #slide3,
#s5:checked ~ #slide4 {
    box-shadow: 0 6px 10px 0 rgba(0,0,0,.3), 0 2px 2px 0 rgba(0,0,0,.2);
    transform: translate3d(-55%,0,-100px);
}

#s1:checked ~ #slide1, #s2:checked ~ #slide2,
#s3:checked ~ #slide3, #s4:checked ~ #slide4,
#s5:checked ~ #slide5 {
    box-shadow: 0 13px 25px 0 rgba(0,0,0,.3), 0 11px 7px 0 rgba(0,0,0,.19);
    transform: translate3d(0,0,0);
}

#s1:checked ~ #slide2, #s2:checked ~ #slide3,
#s3:checked ~ #slide4, #s4:checked ~ #slide5,
#s5:checked ~ #slide1 {
    box-shadow: 0 6px 10px 0 rgba(0,0,0,.3), 0 2px 2px 0 rgba(0,0,0,.2);
    transform: translate3d(55%,0,-100px);
}

#s1:checked ~ #slide3, #s2:checked ~ #slide4,
#s3:checked ~ #slide5, #s4:checked ~ #slide1,
#s5:checked ~ #slide2 {
    box-shadow: 0 1px 4px 0 rgba(0,0,0,.37);
    transform: translate3d(100%,0,-200px);
}

