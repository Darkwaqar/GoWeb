    <script type="text/javascript">
        $(document).ready(function () {
            $('#countdown').countdown('2017/04/28', function (event) {
                $(this).html(event.strftime('%d : %H : %M : %S '));
            });

            function calculateVisibilityForDiv() {
                var div$ = $("#footer-top-top");
                var windowHeight = $(window).height(),
                    docScroll = $(document).scrollTop(),
                    divPosition = div$.offset().top,
                    divHeight = div$.height(),
                    hiddenBefore = docScroll - divPosition,
                    hiddenAfter = (divPosition + divHeight) - (docScroll + windowHeight);

                if ((docScroll > divPosition + divHeight) || (divPosition > docScroll + windowHeight)) {
                    return 0;
                } else {
                    var result = 100;

                    if (hiddenBefore > 0) {
                        result -= (hiddenBefore * 100) / divHeight;
                    }

                    if (hiddenAfter > 0) {
                        result -= (hiddenAfter * 100) / divHeight;
                    }

                    return result;
                }
            }

            function fixDiv() {
                divWidth = $("#fixedSection .container").width() / 4;
                var $cache = $('.t1-rightcolumn');
                var divTop = $("#fixedSection .container:first .row:first").position().top;
                var divBottom = 0;
                if ($(window).scrollTop() > divTop && !(($(document).height() - $(window).scrollTop() - divBottom) <= $(window).height()) && calculateVisibilityForDiv() <= 0) {
                    $cache.css({
                        'position': 'fixed',
                        'top': '0px',
                        'width': divWidth + "px",
                        'height': $(window).height(),
                        'margin-top': '0',
                        'right': (($("#fixedSection").width() - $("#fixedSection .container").width()) / 2) - 8
                    });
                }
                else if (calculateVisibilityForDiv() > 0) {
                    var topPos = $('.t1-rightcolumn').prev().height() - ($('.t1-rightcolumn').height());
                    $cache.css({
                        'position': 'relative',
