@model IList<ProductOverviewModel>
@{
    Layout = "null";

}
@using Nop.Web.Models.Catalog;

        @if (Model.Count > 0)
        {
		
					
                    foreach (var product in Model)
                    {
                        <div class="col-sm-3">
						               
                                        <img src="@Model.AlternatePictureModel.ImageUrl" alt="@Model.DefaultPictureModel.AlternateText">
                                        <p class="text-center">
                                            @product.Name<br>
                                            @product.ProductPrice.Price
                                        </p>
                                    </div>
                    }
                
        }
   