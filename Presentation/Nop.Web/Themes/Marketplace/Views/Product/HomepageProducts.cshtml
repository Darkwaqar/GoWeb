@model List< ProductOverviewModel>
@using Nop.Core
@using Nop.Core.Domain.Orders
@using Nop.Core.Domain.Tax
@using Nop.Core.Infrastructure
@using Nop.Web.Models.Catalog;

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
 foreach (var item in Model)
            {

 <div class="row">
                        <div class="col-md-12">
                            <img src="@item.DefaultPictureModel.ImageUrl" alt="">
                            <a href="@item.SeName"> <h3 class="text-center text-article-right-column">@item.Name</h3></a>
                        </div>
                    </div>
			}								
   
}