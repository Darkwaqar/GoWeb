@model VendorModel

@Html.ValidationSummary(false)
@Html.HiddenFor(model => model.Id)


<script>
    $(document).ready(function () {
        bindBootstrapTabSelectEvent('vendor-edit');
    });
</script>

<div class="content">
    <div class="form-horizontal">
        <div class="row">
            <div class="col-md-12 clearfix">
                <div class="pull-left">
                    @Html.Action("Mode", "Setting", new { modeName = "vendor-advanced-mode" })
                </div>
            </div>
        </div>
        <div id="vendor-edit" class="nav-tabs-custom">
            <ul class="nav nav-tabs">
                @Html.RenderBootstrapTabHeader("tab-info", @T("Admin.Vendors.Info"), true)
                @Html.RenderBootstrapTabHeader("tab-extend-info", @T("Admin.Vendors.Extend.Info"))
                @Html.RenderBootstrapTabHeader("tab-vendor-mobile-setting", @T("Admin.Vendors.VendorMobileAppSetting"))
                @Html.RenderBootstrapTabHeader("tab-vendor-social", @T("Admin.Vendors.Social"))
                @Html.RenderBootstrapTabHeader("tab-vendor-payment-details", @T("Admin.Vendors.PaymentDetails"))
                @Html.RenderBootstrapTabHeader("tab-seo", @T("Admin.Common.SEO"), customCssClass: "advanced-setting")
                @if (Model.Id > 0)
                {
                    @Html.RenderBootstrapTabHeader("tab-vendor-notes", @T("Admin.Vendors.VendorNotes"), customCssClass: "advanced-setting")
                    @Html.RenderBootstrapTabHeader("tab-banner", @T("Admin.Vendors.VendorBannerImageTab"))
                    @Html.RenderBootstrapTabHeader("tab-reviews", @T("Admin.Vendors.ReviewTab"))
                    @Html.RenderBootstrapTabHeader("tab-payouts", @T("Admin.Vendors.PayoutTab"))

                }
            </ul>
            <div class="tab-content">
                @Html.RenderBootstrapTabContent("tab-info", @TabInfo(), true)
                @Html.RenderBootstrapTabContent("tab-extend-info", @TabExtendInfo())
                @Html.RenderBootstrapTabContent("tab-vendor-mobile-setting", @TabMobileAppSettings())
                @Html.RenderBootstrapTabContent("tab-vendor-social", @TabSocialLinks())
                @Html.RenderBootstrapTabContent("tab-vendor-payment-details", @TabPayment())
                @Html.RenderBootstrapTabContent("tab-seo", @TabSeo())
                @if (Model.Id > 0)
                {
                    @Html.RenderBootstrapTabContent("tab-vendor-notes", @TabVendorNotes())
                    @Html.RenderBootstrapTabContent("tab-banner", @TabBannerImages())
                    @Html.RenderBootstrapTabContent("tab-reviews", @TabReviews())
                    @Html.RenderBootstrapTabContent("tab-payouts", @TabPayouts())
                }
            </div>
        </div>
    </div>
</div>

@{
    //custom tabs
    var eventMessage = new AdminTabStripCreated(this.Html, "vendor-edit");
    EngineContext.Current.Resolve<IEventPublisher>().Publish(eventMessage);
    foreach (var eventBlock in eventMessage.BlocksToRender)
    {
    @eventBlock
