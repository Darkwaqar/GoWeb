@helper TabInfo()
{
    
    <script type="text/javascript">
        $(document).ready(function() {
            $("#@Html.FieldIdFor(model => model.AllowCustomersToSelectPageSize)").click(togglePageSize);
            $("#@Html.FieldIdFor(model => model.StandAloneAppAndroid)").click(toggleStandAloneAndroid);
            $("#@Html.FieldIdFor(model => model.StandAloneAppIos)").click(toggleStandAloneIos);
            $("#@Html.FieldIdFor(model => model.MobileAppSettingModel.AppintmentEnable)").click(toggleAppointmentPicture);
            togglePageSize();
            toggleStandAloneAndroid();
            toggleStandAloneIos();
            toggleAppointmentPicture();
        });

        function togglePageSize() {
            if ($('#@Html.FieldIdFor(model => model.AllowCustomersToSelectPageSize)').is(':checked')) {
                $('#pnlPageSize').hide();
                $('#pnlPageSizeOptions').show();
            } else {
                $('#pnlPageSize').show();
                $('#pnlPageSizeOptions').hide();
            }
        }

        function toggleStandAloneAndroid() {
            if ($('#@Html.FieldIdFor(model => model.StandAloneAppAndroid)').is(':checked')) {
                $('#pnlstandaloneandroid').show();
            } else {
                $('#pnlstandaloneandroid').hide();
            }
        }
        function toggleStandAloneIos() {
            if ($('#@Html.FieldIdFor(model => model.StandAloneAppIos)').is(':checked')) {
                $('#pnlstandaloneios').show();
                $('#pnlstandaloneschemesios').show();
            } else {
                $('#pnlstandaloneios').hide();
                $('#pnlstandaloneschemesios').hide();
            }
        }
        function toggleAppointmentPicture() {
            if ($('#@Html.FieldIdFor(model => model.MobileAppSettingModel.AppintmentEnable)').is(':checked')) {
                $('#appointmentPicture').show();
            } else {
                $('#appointmentPicture').hide();
            }
        }
    </script>

    <script type="text/javascript">
            $(document).ready(function() {
                $("#@Html.FieldIdFor(model => model.Address.CountryId)").change(function() {
                    var selectedItem = $(this).val();
                    var ddlStates = $("#@Html.FieldIdFor(model => model.Address.StateProvinceId)");

                    var postData = {
                        countryId: selectedItem,
                        addSelectStateItem: "true"
                    };

                    $.ajax({
                        cache: false,
                        type: "GET",
                        url: "@(Url.Action("GetStatesByCountryId", "Country"))",
                        data: postData,
                        success: function(data) {
                            ddlStates.html('');
                            $.each(data, function(id, option) {
                                ddlStates.append($('<option></option>').val(option.id).html(option.name));
                            });
                        },
                        error: function(xhr, ajaxOptions, thrownError) {
                            alert('Failed to retrieve states.');
                        }
                    });
                });
            });
    </script>

