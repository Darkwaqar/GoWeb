@model FcmSettingsModel
@{

    //page title
    ViewBag.Title = T("Admin.Fcm.Settings").Text;
    //active menu item (system name)
    Html.SetActiveMenuItemSystemName("Fcm Settings");
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Admin.Fcm.Settings")
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateSettings", Model)
}